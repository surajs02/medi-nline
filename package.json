    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@types/cheerio": "^0.22.24",
    "@types/node": "^14.14.31",
    "@types/source-map-support": "^0.5.3",
    "concurrently": "^6.0.0",
    "eslint": "^7.20.0",
